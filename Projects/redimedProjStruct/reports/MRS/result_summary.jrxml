	<queryString>
		<![CDATA[select concat( ifnull(p.`First_name`,''),' ' ,ifnull(p.`Sur_name`,'')) as full_name , p.`DOB` , ifnull(p.`Address1`, p.`Address2`) as Address , concat(  ifnull(p.`Suburb` , ''),'/',ifnull(p.`State`,''),'/', ifnull( p.`Post_code`,''))  as code , p.`Sex`   , p.`Country`  ,p.`Mobile` , p.`Occupation` , h.`practitioner` , SUBSTRING_INDEX(h.`practitionSign`, ',', -1) as practitionSign , h.`practitionDate` , h.`CAL_ID` , h.`MRS_DF_ID` , h.`PATIENT_ID` , ifnull(isReview,0) as isReview from `cln_mrs_headers` h inner join `cln_patients` p on p.`Patient_id` = h.`PATIENT_ID` where h.cal_id = $P{cal_id} and h.mrs_df_id = $P{key} and h.patient_id = $P{patient_id}]]>
	</queryString>
