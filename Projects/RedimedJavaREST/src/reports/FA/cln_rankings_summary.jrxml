	<parameter name="ranking_summary_header_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
concat(ranking.FROM_VALUE,'-',ranking.TO_VALUE) as VALUE_RANGE,
ranking.RATE,
header.DESCRIPTION
 from `sys_rankings` ranking
 inner join `sys_ranking_headers` header on ranking.HEADER_ID = header.RATING_ID
where ranking.HEADER_ID =  $P{ranking_summary_header_id}
group by ranking.RATE]]>
	</queryString>
