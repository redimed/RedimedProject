	<queryString>
		<![CDATA[select DISTINCT 	header.*,patient.`First_name`,patient.`Sur_name`,patient.`Middle_name`,	patient.`DOB`, patient.`Address1`,
YEAR(CURDATE())- YEAR(patient.DOB) AS PATIENT_AGE, SUBSTRING_INDEX(header.`ASSESSED_SIGN`, ',', -1) AS  'ASSESSED_SIGN_1',
SUBSTRING_INDEX(header.`PATIENT_SIGN`, ',', -1) AS  'PATIENT_SIGN_1', company.`Company_name`
from 	`cln_fa_df_headers` header
left join `cln_patients` patient on header.`PATIENT_ID`=patient.`Patient_id`
left join `companies` company on patient.`company_id` = company.`id`
where 	header.`ISENABLE`=1
	and
	header.`PATIENT_ID`=$P{patient_id}
	and
	header.`CAL_ID`=$P{cal_id}
	and
	header.`FA_ID`=$P{key}]]>
	</queryString>
