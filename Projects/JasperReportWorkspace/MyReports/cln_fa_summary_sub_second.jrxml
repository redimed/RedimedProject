	<queryString>
		<![CDATA[SELECT DISTINCT header.Risk,
	header.Comments,
	header.Recommend,
	header.Att_Flexibility,
	header.Att_Core_Stability,
	header.Att_Wirst_Elbow_func,
	header.Att_Shoulder_func,
	header.Att_Lower_Limb_func,
	header.Att_Balance,
	header.ASSESSED_ID,
	SUBSTRING_INDEX(header.ASSESSED_SIGN, ',', -1) AS ASSESSED_SIGN,
	header.ASSESSED_DATE,
	header.ASSESSED_NAME
	FROM cln_fa_df_headers header
	WHERE header.ISENABLE = 1
 	AND header.CAL_ID = $P{cal_id}
 	AND header.PATIENT_ID = $P{patient_id}
 	AND header.FA_ID = $P{key}]]>
	</queryString>
